{
  "extends": ["wesbos"],
  "rules": {
    "react/prop-types": 1,
    "camelcase": [1, { "ignoreDestructuring": true }],
    "no-console": 2,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 8,
      }
    ]
  }
}
