{
  "extends": ["wesbos"],
  "rules": {
    "no-console": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 80,
        "tabWidth": 2,
        "endOfLine" : "auto",
        "no-use-before-define": "false"
      }
    ]
  }
}
